Private Sub LanceDemo(Active As Boolean)

    DoCmd.Hourglass Active
    Me.txtTmp.SetFocus
    Me.cmdCloseFrm.Enabled = Not Active
    Me.cmdTestCls.Enabled = Not Active

    If (Not Active) Then
        Set cDemo = Nothing
        Set cLabels = Nothing
    Else
        Set cDemo = New C_Demo2
    End If

End Sub
