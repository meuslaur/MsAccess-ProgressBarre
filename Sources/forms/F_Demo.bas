    Private cLabels  As C_LabelsInfo                                        '// Code C_LabelsInfo

Private Sub Form_Close()
    Set cDemo = Nothing
    Set cLabels = Nothing                                                   '// Code C_LabelsInfo
End Sub

Private Sub cmdCloseFrm_Click()
    DoCmd.Close
End Sub

Private Sub cmdTestCls_Click()

    InitLabels
    LanceDemo True

    Set cLabels = cDemo.GetClsLabelsInfo()                                  '// Code C_LabelsInfo
    cLabels.InitialiseLabels b1, t1, b2, t2                                 '// Code C_LabelsInfo
    cLabels.SetLengthBarre Me.zlCarB1, Me.txtBoucle                         '// Code C_LabelsInfo

    cDemo.BoucleTestCls (Me.txtBoucle)
    LanceDemo False

End Sub


Private Sub cmdTestFrm_Click()
    InitLabels
    LanceDemo True

    Set cLabels = New C_LabelsInfo                                          '// Code C_LabelsInfo
    cLabels.InitialiseLabels b1, t1, b2, t2                                 '// Code C_LabelsInfo
    cLabels.SetLengthBarre Me.zlCarB1, Me.txtBoucle                         '// Code C_LabelsInfo

    BoucleDemo Me.txtBoucle
    cLabels.HiddenCurrentLabels                                             '// Code C_LabelsInfo
    LanceDemo False

End Sub

Private Sub BoucleDemo(Boucle As Long)
    '//---------Demo-----
    Dim i As Long
    Dim lRand As Long
    Dim sAvance As String

    For i = 1 To Boucle
        lRand = Int((700 * Rnd) + 1)
        sAvance = "Boucle 1 : " & CStr(i)
        cLabels.UpdateBarre sAvance                                         '// Code C_LabelsInfo
        If (i = Me.txtBoucle2Dep) Then
            cLabels.SetLengthBarre Me.zlCarB2, Me.txtBoucle2, LabelNum2     '// Code C_LabelsInfo
            BoucleDemo2 Me.txtBoucle2
            cLabels.ReturnFirstBarre                                        '// Code C_LabelsInfo
        End If
        DoEvents                        '// Refresh labels.
        Sleep (lRand)
   Next i

End Sub

Private Sub BoucleDemo2(Boucle As Long)
    Dim i As Long
    Dim lRand As Long
    Dim sAvance As String

    For i = 1 To Boucle
        lRand = Int((700 * Rnd) + 1)
        sAvance = "Boucle 2 : " & CStr(i)
        cLabels.UpdateBarre sAvance                                         '// Code C_LabelsInfo
        DoEvents                        '// Refresh labels.
        Sleep (lRand)
   Next i
End Sub

