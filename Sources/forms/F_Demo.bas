Private Sub LanceDemo(Active As Boolean)


'    Me.lbl_TexteInfo.Visible = Active
'    Me.lbl_BarreInfo.Visible = Active
    DoCmd.Hourglass Active

    Me.txtBoucle.SetFocus
    Me.cmdCloseFrm.Enabled = Not Active
    Me.cmdTestCls.Enabled = Not Active
    Me.cmdTestFrm.Enabled = Not Active

    If (Not Active) Then
        Me.lbl_TexteInfo.Caption = "Fin"
        Me.lbl_BarreInfo.Caption = vbNullString
        ResetCls
    Else
        Set cDemo = New C_Demo
    End If

End Sub

Private Sub ResetCls()
    Set cDemo = Nothing
    Set cLabels = Nothing
End Sub

Private Sub txtBoucle2Dep_AfterUpdate()
    Me.txtBoucle2Dep = IIf(Me.txtBoucle2Dep > Me.txtBoucle, 0, Me.txtBoucle2Dep)
End Sub

Private Sub InitLabels()
    Set b1 = IIf(Me.chkB1, Me.lbl_BarreInfo, Nothing)
    Set t1 = IIf(Me.chkT1, Me.lbl_TexteInfo, Nothing)
    Set b2 = IIf(Me.chkB2, Me.lbl_BarreInfo2, Nothing)
    Set t2 = IIf(Me.chkT2, Me.lbl_TexteInfo2, Nothing)
End Sub
